#%RAML 1.0 DataType
type: array
items:
  type: object
  properties:
    companyName:
      required: true
      type: string
      example: "TravelOnTip"
    availableSeats:
      required: true
      type: number
      example: 20
    departureDateTime:
      required: true
      type: string
      example: "2019-04-30T16:41:41"
    travelRoute:
      type: object
      properties:
        destinationLocation:
          type: object
          properties:
            locationCode:
              required: true
              type: string
              example: "KLG-MY"
            locationDesc:
              required: true
              type: string
              example: "Klang Malaysia"
        originLocation:
          type: object
          properties:
            locationCode:
              required: true
              type: string
              example: "BEN-SG"
            locationDesc:
              required: true
              type: string
              example: "Beauna Vista Singapore"